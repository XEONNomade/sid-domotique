<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/domotique</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">loubna</property>
    <mapping resource="classes/Champs.hbm.xml"/>
    <mapping resource="classes/Analyse.hbm.xml"/>
    <mapping resource="classes/Countrycodes.hbm.xml"/>
    <mapping resource="classes/Analysechamp.hbm.xml"/>
    <mapping resource="classes/Items.hbm.xml"/>
    <mapping resource="classes/Coocurrences.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
